def file(file):
    for l in file:
        line = l.strip()
        data = split(line)
        yr = read_year(data)
        minmax = find_minmax(yr)
        
    return (yr,minmax)

for l in f2009:
        line = l.strip()
        data = split(line)
        yr = read_year(data)
        minmax = find_minmax(data)
        print(yr,minmax)
