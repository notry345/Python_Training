def split(line):
    date,rate = line.split("          ")
    y,m,d = date.split("/")
    Date = int(y)*10000+int(m)*100+int(d)
    Rate = 1//float(rate)
    data = (Date,Rate)
    return data
def read_year(data):
    (date,rate) = data
    yr = date//1000
    return yr
def find_minmax(data):
    minmax = [(9999,0)]*12
    for d,v in data:
        month = (d//100)%100-1
        minr,maxr = minmax[month]
        if v<minr:
            minr = v
        if v>maxr:
            maxr = v
        minmax[month] = minr,maxr
    return minmax
